<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="#fff"
    android:id="@+id/activity_persional_jiahao_item"
    android:orientation="horizontal" android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <ImageView
        android:layout_width="55dp"
        android:layout_height="60dp"
        android:id="@+id/activity_persional_jiahao_item_image"
        android:layout_margin="10dp"
        />
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:layout_margin="5dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/activity_persional_jiahao_item_name"
            />
        <TextView
            android:layout_margin="5dp"
            android:layout_below="@id/activity_persional_jiahao_item_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/activity_persional_jiahao_item_time"
            />
        <TextView
            android:layout_margin="5dp"
            android:layout_below="@id/activity_persional_jiahao_item_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/activity_persional_jiahao_item_shenhe"
            />
        <TextView
            android:layout_margin="5dp"
            android:layout_below="@id/activity_persional_jiahao_item_shenhe"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/activity_persional_jiahao_item_result"
            />
    </RelativeLayout>
</LinearLayout>